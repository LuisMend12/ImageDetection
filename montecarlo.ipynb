    "import numpy as np  \n",
    "import matplotlib.pyplot as plt \n",
    "\n",
    "\n",
    "a = 0\n",
    "b = 0\n",
    "\n",
    "x = np.linspace(a, b, 1000)\n",
    "\n",
    "\n",
    "pdf_values  = np.full_like(x, 1 / (b - a))\n",
    "\n",
    "\n",
    "plt.plot(x, pdf_values)\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('Probability Density')\n",
    "plt.title('Uniform Distribution PDF')\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
  }
